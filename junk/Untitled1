    <button class="btn2">Slide Down</button>




$(document).ready(function(){   
    $('.btn1').click(function(){
        $('.igiveup').slideUp();
    });

    $('.btn2').click(function(){
        $('.igiveup').slideDown();
        
        
    });
});









